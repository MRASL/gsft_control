<launch>
	<node pkg="gsft_control" type="scenario2_gs_controller_node" name="scenario2_gs_controller" output="screen">
		<rosparam command="load" file="$(find gsft_control)/cfg/firefly.yaml" />
		<remap from="odometry" to="/msf_core/odometry" />
		<remap from="command/motor_rpm" to="/fcu/command/direct_motor" />
	</node>
</launch>
